@page
@model UCS.App.Frontend.Pages.EliminarPersonalAseoModel
@{
}
<h1>Se va a Eliminar el Personal de Aseo</h1>

<table class="table">

    <tr>
        <td>Nombre:</td>
        <td>@Model.PersonalAseo.nombre</td>
    </tr>
    <tr>
        <td>Apellido:</td>
        <td>@Model.PersonalAseo.apellido</td>
    </tr>
    <tr>
        <td>Identificación:</td>
        <td>@Model.PersonalAseo.identificacion</td>
    </tr>
    <tr>
        <td>Edad:</td>
        <td>@Model.PersonalAseo.edad</td>
    <tr></tr>
    <tr>
        <td>Estado Covid:</td>
        <td>@Model.PersonalAseo.EstadoCovid</td>
    </tr>
    <tr>
        <td>Unidad Desempeña:</td>
        <td>@Model.PersonalAseo.turnoServicio</td>
    </tr>

</table>
<form method="POST">
    <input type="hidden" asp-for="PersonalAseo.id">
    <button type="submit" class="btn btn-primary">Confirmar Eliminar</button>

</form>